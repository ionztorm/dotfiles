{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#fab387",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
         "foreground": "#89b4fa",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "git",
          "foreground": "#eba0ac",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#b4befe>git:(</>{{ .HEAD }}<#b4befe>)</>"
        },
        {
          "foreground": "#94e2d5",
          "style": "plain",
          "template": "{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}",
          "type": "git",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          }
        },
        {
          "type": "python",
          "foreground": "#f9e2af",
          "style": "plain",
          "properties": {
            "display_mode": "context"
          },
          "template": "{{ if .Venv }} Óúº {{ end }}"
        },
        {
          "foreground": "#BF616A",
          "style": "plain",
          "template": " \u2717",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
